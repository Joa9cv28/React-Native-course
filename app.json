    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#24180f",
      "bundleIdentifier": "com.joanine.RNCourse"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
